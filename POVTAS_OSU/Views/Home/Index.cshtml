<style>
    .wrapper-carousel {
        width: 100%;
        height: 300px;
    }

    .carousel-img-1 {
        background-image: url(../../Images/PC.jpg);
        background-repeat: no-repeat;
        background-position: 0px 0px;
        background-size: cover;
    }

    .carousel-img-2 {
        background-image: url(../../Images/Main1.jpg);
        background-repeat: no-repeat;
        background-position: 0px 0px;
        background-size: cover;
    }

    .carousel-img-3 {
        background-image: url(../../Images/olimpiada.jpg);
        background-repeat: no-repeat;
        background-position: 0px 0px;
        background-size: cover;
    }

    .wrapper-body-img {
        width: 100%;
        height: 100%;
    }


    .body-img-1 {
        background-image: url(../../Images/OSU1.png);
        background-repeat: no-repeat;
        background-position: 0px 0px;
        background-size: cover;
    }

    .body-img-2 {
        background-image: url(../../Images/olimpiada.jpg);
        background-repeat: no-repeat;
        background-position: 0px 0px;
        background-size: cover;
    }

    .body-img-3 {
